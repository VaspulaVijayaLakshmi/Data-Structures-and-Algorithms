class Solution {
public:
    int evalRPN(vector<string>& s) {
      stack<int> stk;
        int ans=0;
        
       
        for(int i=0;i<s.size();i++) {
            
            if(s[i]=="+"){
                
                int a=stk.top();
                stk.pop();
                int b=stk.top();
                stk.pop();
                
                stk.push(a+b);
             
            }
                
            else if(s[i]=="-"){
                 int a=stk.top();
                  stk.pop();
                int b=stk.top();
                  stk.pop();
                
                stk.push(b-a);
               
            }
             
            else if(s[i]=="*"){
                 int a=stk.top();
                  stk.pop();
                int b=stk.top();
                  stk.pop();
                
                stk.push(a*b);
            }
                
                
           else if(s[i]=="/"){
                int a=stk.top();
                 stk.pop();
                int b=stk.top();
                 stk.pop();
                
                stk.push(b/a);
             
           }
               
           else{
               stk.push(stoi(s[i]));
           }    
                        
        }
        
        
        return stk.top();
    }
};





